PORT=4000
NODE_ENV=development
DATABASE_URL=sqlite:./dev.sqlite
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1h
CORS_ORIGIN=http://localhost:3000

# Rate limiter settings (milliseconds and counts)
# AUTH_RATE_WINDOW_MS: window in ms for auth endpoints (default 15 minutes)
# AUTH_RATE_MAX: max requests per window for auth endpoints (default 20)
AUTH_RATE_WINDOW_MS=900000
AUTH_RATE_MAX=20

# GLOBAL_RATE_WINDOW_MS and GLOBAL_RATE_MAX for other endpoints
GLOBAL_RATE_WINDOW_MS=900000
GLOBAL_RATE_MAX=100
